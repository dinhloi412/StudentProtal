@{
}

@model StudentProtalWeb.Models.AddStudentViewModel
<form method="post">
    <h1>Student</h1>
    <div class="mt-3">
        <label class="form-label">Name</label>
        <input  type="text" class="form-control" asp-for="Name"/>
    </div>

    <div class="mt-3">
        <label class="form-label">Email</label>
        <input type="text" class="form-control" asp-for="Email" />
    </div>

    <div class="mt-3">
        <label class="form-label">Phone</label>
        <input type="text" class="form-control" asp-for="Phone" />
    </div>

    <div class="mt-3">
        <input type="checkbox" class="form-check-input" id="Subcribed" asp-for="Subcribed" />
        <label class="form-check-label" for="Subcribed">Subcribed</label>
    </div>


    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>
